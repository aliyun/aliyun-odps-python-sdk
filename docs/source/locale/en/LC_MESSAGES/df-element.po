#: ../../source/df-element.rst:225
msgid "rstrip"
msgstr ""

#: ../../source/df-element.rst:225
msgid "在右侧删除空格（包括空行符）"
msgstr "Remove spaces (including blank lines) on the right of the string."

#: ../../source/df-element.rst:226
msgid "strip"
msgstr ""

#: ../../source/df-element.rst:226
msgid "在左右两侧删除空格（包括空行符）"
