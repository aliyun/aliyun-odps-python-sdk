                <docs>中文日期合并为一个语意单元</docs>
            </param>
            <param name="enableChnTimeMerge">
                <value>false</value>
                <docs>中文时间合并为一个语意单元</docs>
            </param>
            <param name="tokenizer">
                <value>TAOBAO_CHN</value>
                <docs>分类器类型</docs>
            </param>
            <param name="enableSemanticTagger">
